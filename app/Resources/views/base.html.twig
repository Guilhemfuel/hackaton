<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8" />
    <title>{% block title %}Albert{% endblock %}</title>
    {% block stylesheets %}{% endblock %}
    <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
    <link rel="stylesheet" type="text/css" href="css/style.css">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link href="https://fonts.googleapis.com/css?family=Lato" rel="stylesheet">
</head>
<body>
<div id="container">
    <div id="body">
        {% block body %}{% endblock %}
        {% block prefooter %}

        {% endblock %}
    </div>

    {% block footer %}
        {% if app.request.get('_route') != 'homepage' %}
            <div id="prefooter">
                <div><a href="{{ path('homepage') }}"><img src="{{ asset('bundles/app/images/m_home.png') }}"/></a>
                </div>
                <div><a href="{{ path('activities') }}"><img src="{{ asset('bundles/app/images/m_rencontres.png') }}"/></a></div>
                <div><a href="{{ path('docteur') }}"><img src="{{ asset('bundles/app/images/m_medecins.png') }}"/></a></div>
                <div><a href="{{ path('questionnaire') }}"><img
                                src="{{ asset('bundles/app/images/m_questionnaire.png') }}"/></a></div>
                <div><a href="{{ path('register') }}"><img src="{{ asset('bundles/app/images/m_doc.png') }}"/></a>
                </div>
            </div>
        {% endif %}
        <footer>
            <a>Qui sommes nous ?</a>
            <a class="middle">CGU</a>
            <a>Mentions l√©gales</a>
        </footer>
    {% endblock %}
</div>

{% block javascripts %}{% endblock %}
</body>
</html>